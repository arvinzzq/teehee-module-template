{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "author": "{{{ author }}}",
  "license": "{{ license }}",
  "scripts": {
    "dev": "node_modules/.bin/babel docs/src -d docs/dist --watch",
    "build": "node_modules/.bin/babel src -d lib",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    {{#each keywords}}
      "{{this}}"{{#if @last}}{{else}},{{/if}}
    {{/each}}
    ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "dependencies": {}
}
